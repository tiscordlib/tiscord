{
  "name": "tiscord",
  "version": "2.0.0",
  "description": "A fast, lightweight, and easy to use Discord API wrapper",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "format": "biome format ./src",
    "lint": "biome lint ./src",
    "test": "npx vitest --run",
    "prepublishOnly": "npm run build",
    "docs": "npx jsdoc --configure ./jsdoc.json --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiscordlib/tiscord.git"
  },
  "author": "xiboon & meetzli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tiscordlib/tiscord/issues"
  },
  "homepage": "https://tiscord.com",
  "dependencies": {
    "@sapphire/async-queue": "^1.5.2",
    "discord-api-types": "^0.37.66",
    "form-data": "^4.0.0",
    "typescript": "^5.4.2",
    "undici": "^6.7.0",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/node": "^18.0.0",
    "@types/ws": "^8.5.0",
    "@vitest/coverage-c8": "^0.23.4",
    "clean-jsdoc-theme": "^4.2.17",
    "jsdoc": "^4.0.2",
    "tsup": "^8.0.2",
    "vitest": "^0.23.4"
  }
}