{
	"name": "tiscord",
	"version": "2.0.0",
	"description": "A fast, lightweight, and easy to use Discord API wrapper",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"typecheck": "tsc --noEmit",
		"format": "biome format . --write",
		"check": "biome check .",
		"lint": "biome lint .",
		"test": "vitest run",
		"docs": "jsdoc --configure ./jsdoc.json --verbose"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tiscordlib/tiscord.git"
	},
	"author": "xiboon & meetzli",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/tiscordlib/tiscord/issues"
	},
	"homepage": "https://tiscord.com",
	"dependencies": {
		"@sapphire/async-queue": "^1.5.2",
		"discord-api-types": "^0.37.73",
		"form-data": "^4.0.0",
		"typescript": "^5.4.2",
		"undici": "^6.7.1",
		"ws": "^8.16.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.6.0",
		"@types/node": "^20.11.25",
		"@types/ws": "^8.5.10",
		"@vitest/coverage-v8": "^1.3.1",
		"clean-jsdoc-theme": "^4.2.17",
		"jsdoc": "^4.0.2",
		"tsup": "^8.0.2",
		"vitest": "^1.3.1"
	},
  "engines": {
    "node": ">=18.0.0"
  },
	"packageManager": "pnpm@8.15.4"
}
